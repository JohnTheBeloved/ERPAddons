<odoo><!--Product Listing page-->
	<template id="diza_products" inherit_id="website_sale.products" name="Products Listing Grid">
		<data id="diza_products" inherit_id="website_sale.products" name="Products Listing Grid">
			<xpath expr="//table" position="replace">
				<div id="main-wrapper" class="shop-left main-wrapper ">
					<section id="tbay-breadscrumb" class="tbay-breadscrumb  breadcrumbs-image">
						<img width="1920" height="200" src="/theme_diza/static/img/diza/07/image-background-woo.jpg" class="attachment-full size-full" alt="breadscrumb">
							<div class="container">
								<div class="breadscrumb-inner">
									<h1 class="page-title">Shop</h1>
									<ol class="tbay-woocommerce-breadcrumb breadcrumb">
										<li>
											<a href="https://elementor.thembay.com/diza_medicine">Home</a>
										</li>
										<li>
											<a href="https://elementor.thembay.com/diza_medicine/shop/">Shop</a>
										</li>
										<li>Page 2</li>
									</ol>
								</div>
							</div>
						</img>
					</section>
					<div id="main-container" class="container">
						<div class="row">
							<div id="products_side_filter"/>
							<div id="main" class="archive-shop col-12 col-xl-9 content col-12">
								<header class="woocommerce-products-header"/>
								<div id="products_top_filter"/>
								<div class="display-products products products-grid" style="display: block;">
									<div class="row" data-xlgdesktop="4" data-desktop="4" data-desktopsmall="4" data-tablet="3" data-landscape="3" data-mobile="2">
										<t t-foreach="products" t-as="product">
											<t t-call="website_diza.diza_product_list_item"/>
										</t>
									</div>
								</div>
								<div id="products_bottom_pagination"/>
							</div>
						</div>
					</div>
				</div>
			</xpath>
		</data>
		<data id="diza_restyle_layout" inherit_id="website_sale.products" name="Remove Attributes">
			<xpath expr="//div[@id='products_grid']" position="attributes">
			 	<attribute name="class"></attribute>
			</xpath>
		</data>
		<data id="diza_restyle_layout" inherit_id="website.layout" name="Restyle Layout Attributes">
			<xpath expr="//div[hasclass('oe_website_sale')]" position="attributes">
			 	<attribute name="class"></attribute>
			</xpath>
		</data>
	</template>
